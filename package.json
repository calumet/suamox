{
  "name": "suamox",
  "version": "0.1.0",
  "description": "Meta-framework with SSR/SSG, filesystem routing, and islands architecture",
  "private": true,
  "type": "module",
  "bin": {
    "suamox": "./bin/create-suamox.js",
    "create-suamox": "./bin/create-suamox.js"
  },
  "scripts": {
    "build": "pnpm --filter \"@suamox/*\" build",
